  "version": "1.0.1",
  "authors": [
    {
      "name": "Seth Battis",
      "email": "sbattis@groton.org"
    }
  ],
  "license": "GPL-3.0",
  "require": {
    "google/cloud-firestore": "^1.48",
    "groton-school/slim-lti-shim": "^1.0",
    "packbackbooks/lti-1p3-tool": "^6.2"
  },
  "autoload": {
